(dirs :standard \ forbidden_var)

(alias
 (name x)
 (action (echo "Building alias x"))
 (enabled_if false))

(alias
 (name y)
 (action (echo "Building alias y"))
 (enabled_if true))

(rule
 (targets a)
 (action (progn
          (echo "Building file a")
          (with-stdout-to a (progn))))
 (enabled_if false))

(rule
 (enabled_if true)
 (targets b)
 (action (progn
          (echo "Building file b")
          (with-stdout-to b (progn)))))

(library
 (name foo)
 (modules)
 (enabled_if false))

(library
 (name bar)
 (modules)
 (libraries foo))

(library
 (name baz)
 (modules)
 (libraries bar))

(rule (with-stdout-to main.ml (echo "")))

(executable (name main) (libraries baz))
