(library
 (name a)
 (libraries b)
 (modules))

(library
 (name b)
 (libraries c)
 (modules))

(library
 (name c)
 (libraries a)
 (modules))

(executable
 (name cycle)
 (libraries a)
 (modules cycle))

(rule (with-stdout-to cycle.ml (echo "")))

(executable
 (name select_error)
 (libraries (select x from))
 (modules select_error))

(rule (with-stdout-to select_error.ml (echo "")))
